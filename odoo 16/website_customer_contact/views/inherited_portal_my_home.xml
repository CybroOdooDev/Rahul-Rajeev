<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="website_customer_contact_portal_contact" name="Portal Contacts" customize_show="True" inherit_id="portal.portal_my_home"
              priority="100">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Contacts</t>
                <t t-set="url" t-value="'/my/contacts'"/>
                <t t-set="placeholder_count" t-value="'contact_count'"/>
            </t>
        </xpath>
    </template>
</odoo>